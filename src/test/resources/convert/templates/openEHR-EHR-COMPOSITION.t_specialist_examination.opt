                                    <items id="text">Дерево</items>
                                </term_definitions>
                                <term_definitions code="at0013">
                                    <items id="description">*The body position of the subject during the observation.(en)</items>
                                    <items id="text">Положение</items>
                                </term_definitions>
                                <term_definitions code="at1000">
                                    <items id="description">*The subject was lying flat.(en)</items>
                                    <items id="text">лежа</items>
                                </term_definitions>
                                <term_definitions code="at1001">
                                    <items id="description">*The subject was sitting (for example on bed or chair).(en)</items>
                                    <items id="text">сидя</items>
                                </term_definitions>
                                <term_definitions code="at1002">
                                    <items id="description">*The subject was reclining at an approximate angle of 45 degrees, with the legs elevated to the level of the pelvis.(en)</items>
                                    <items id="text">под наклоном</items>
                                </term_definitions>
                                <term_definitions code="at1003">
                                    <items id="description">*The subject was standing, walking or running.(en)</items>
                                    <items id="text">стоя/вертикально</items>
                                </term_definitions>
                                <term_definitions code="at1005">
                                    <items id="comment">*It can be implied that the pulse is present if Rate &gt;0 beats/min.(en)</items>
                                    <items id="description">Наличие пульса или сердцебиения.</items>
                                    <items id="text">Наличие пульса или сердцебиения</items>
