                                <term_definitions code="at0000">
                                    <items id="description">*Record details about the rate and associated attributes for a pulse or heart beat.(en)</items>
                                    <items id="text">Пульс/ЧСС</items>
                                </term_definitions>
                                <term_definitions code="at0001">
                                    <items id="description">@ внутреннийl @</items>
                                    <items id="text">Дерево</items>
                                </term_definitions>
                                <term_definitions code="at0002">
                                    <items id="description">@ внутреннийl @</items>
                                    <items id="text">История</items>
                                </term_definitions>
                                <term_definitions code="at0003">
                                    <items id="description">*Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.(en)</items>
                                    <items id="text">Любое событие</items>
                                </term_definitions>
                                <term_definitions code="at0004">
                                    <items id="comment">*Run-time name constraints have been specified, in order to simplify the renaming of this data element to Pulse Rate or Heart Rate, as required.(en)</items>
                                    <items id="description">Частота (пульса/сердечных сокращений), измеряемая в уд/мин.</items>
                                    <items id="text">Частота</items>
                                </term_definitions>
                                <term_definitions code="at0005">
                                    <items id="description">*Is the pulse or heart beat regular?(en)</items>
                                    <items id="text">Ритм</items>
                                </term_definitions>
                                <term_definitions code="at0006">
                                    <items id="description">*The pattern is regular.(en)</items>
                                    <items id="text">правильный</items>
                                </term_definitions>
                                <term_definitions code="at0007">
                                    <items id="description">*The pattern is irregular in a regular pattern,. For example, a dropped beat once every 'n' beats.(en)</items>
                                    <items id="text">регулярный</items>
                                </term_definitions>
                                <term_definitions code="at0008">
                                    <items id="description">*The pattern is irregular in a chaotic and unpredictable manner. For example, atrial fibrillation.(en)</items>
                                    <items id="text">нерегулярный</items>
                                </term_definitions>
                                <term_definitions code="at0010">
                                    <items id="description">@ внутреннийl @</items>
                                    <items id="text">Дерево</items>
                                </term_definitions>
                                <term_definitions code="at0012">
                                    <items id="description">@ внутреннийl @</items>
                                    <items id="text">Дерево</items>
                                </term_definitions>
                                <term_definitions code="at0013">
                                    <items id="description">*The body position of the subject during the observation.(en)</items>
                                    <items id="text">Положение</items>
                                </term_definitions>
                                <term_definitions code="at1000">
                                    <items id="description">*The subject was lying flat.(en)</items>
                                    <items id="text">лежа</items>
                                </term_definitions>
                                <term_definitions code="at1001">
                                    <items id="description">*The subject was sitting (for example on bed or chair).(en)</items>
                                    <items id="text">сидя</items>
                                </term_definitions>
                                <term_definitions code="at1002">
                                    <items id="description">*The subject was reclining at an approximate angle of 45 degrees, with the legs elevated to the level of the pelvis.(en)</items>
                                    <items id="text">под наклоном</items>
                                </term_definitions>
                                <term_definitions code="at1003">
                                    <items id="description">*The subject was standing, walking or running.(en)</items>
                                    <items id="text">стоя/вертикально</items>
                                </term_definitions>
                                <term_definitions code="at1005">
                                    <items id="comment">*It can be implied that the pulse is present if Rate &gt;0 beats/min.(en)</items>
                                    <items id="description">Наличие пульса или сердцебиения.</items>
                                    <items id="text">Наличие пульса или сердцебиения</items>
                                </term_definitions>
                                <term_definitions code="at1013">
                                    <items id="description">*Details about the device used to measure the pulse rate or heart rate.(en)</items>
                                    <items id="text">Устройство</items>
                                </term_definitions>
                                <term_definitions code="at1017">
                                    <items id="description">*Details about physical exertion being undertaken during the examination.(en)</items>
                                    <items id="text">Дополнительно</items>
                                </term_definitions>
                                <term_definitions code="at1018">
                                    <items id="comment">*For example, presence of a pacemaker, level of anxiety; pain or fever etc.(en)</items>
                                    <items id="description">*Narrative description about any incidental factors that may affect interpretation of the physical findings.(en)</items>
                                    <items id="text">Искажающие факторы</items>
                                </term_definitions>
                                <term_definitions code="at1019">
                                    <items id="comment">*For example, auscultation or electronic monitoring. (en)</items>
                                    <items id="description">*Method used to observe the pulse or heart beat.(en)</items>
                                    <items id="text">Метод</items>
                                </term_definitions>
                                <term_definitions code="at1022">
                                    <items id="description">*Narrative description about the pulse or heart beat.(en)</items>
                                    <items id="text">Клиническое описание</items>
                                </term_definitions>
                                <term_definitions code="at1023">
                                    <items id="comment">*Coding with a terminology is preferred, where possible. For example: Bradycardia, Extrasystoles or Sinus rhythm. Multiple statements are allowed. (en)</items>
                                    <items id="description">*Single word, phrase or brief description that represents the clinical meaning and significance of the pulse or heart beat findings, including the rhythm.(en)</items>
                                    <items id="text">Клиническая интерпретация результатов</items>
                                </term_definitions>
                                <term_definitions code="at1024">
                                    <items id="description">Пульс/Сердцебиение определяется</items>
                                    <items id="text">определяется</items>
                                </term_definitions>
                                <term_definitions code="at1025">
                                    <items id="description">Пульс/Сердцебиение не определяется</items>
                                    <items id="text">не определяется</items>
                                </term_definitions>
                                <term_definitions code="at1026">
                                    <items id="description">Частота пульса, измеряемая в уд/мин</items>
                                    <items id="text">Пульс</items>
                                </term_definitions>
                                <term_definitions code="at1027">
                                    <items id="description">Частота сердечных сокращений, измеряемая в уд/мин</items>
                                    <items id="text">ЧСС</items>
                                </term_definitions>
                                <term_definitions code="at1028">
                                    <items id="description">*The pattern is irregular.(en)</items>
                                    <items id="text">неправильный</items>
                                </term_definitions>
                                <term_definitions code="at1030">
                                    <items id="comment">*Coding with a terminology is desired, where possible. For example: full, thready, bounding, slow rising, or collapsing. Multiple terms may be recorded.(en)</items>
                                    <items id="description">*Description of the character of the pulse or heart beat.(en)</items>
                                    <items id="text">Характер пульса</items>
                                </term_definitions>
                                <term_definitions code="at1032">
                                    <items id="description">*The findings are observed by physical touch of the observer on the subject.(en)</items>
                                    <items id="text">пальпация</items>
                                </term_definitions>
                                <term_definitions code="at1033">
                                    <items id="description">*The findings are observed with the assistance of a device, such as a stethoscope.(en)</items>
                                    <items id="text">аускультация</items>
                                </term_definitions>
                                <term_definitions code="at1034">
                                    <items id="description">*The findings are observed non-invasively using a device such as a pulse oximeter or a stethoscope.(en)</items>
                                    <items id="text">спонтанный, не инвазивный</items>
                                </term_definitions>
                                <term_definitions code="at1036">
                                    <items id="description">*Maximum pulse rate or heart rate observed during a period of exertion.(en)</items>
                                    <items id="text">Максимальный</items>
                                </term_definitions>
                                <term_definitions code="at1037">
                                    <items id="description">*Body site where the pulse or heart beat were observed.(en)</items>
                                    <items id="text">Локализаця на теле</items>
                                </term_definitions>
