                                    <items id="text">мочка уха</items>
                                </term_definitions>
                                <term_definitions code="at1054">
                                    <items id="description">*An unspecified toe.(en)</items>
                                    <items id="text">палец ноги</items>
                                </term_definitions>
                                <term_definitions code="at1055">
